	</xs:attributeGroup>
	<xs:attributeGroup name="att.common.attribute.n">
		<xs:attribute name="n" type="xs:string">
			<xs:annotation>
				<xs:documentation>provides
          a label (name or number) for an element. While its value need not be
          unique, it is required to be a single token.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="att.controlevent.attributes">
