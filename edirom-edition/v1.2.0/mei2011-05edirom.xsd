			<xs:element ref="syllable"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="model.syllableLike_alternation">
		<xs:sequence>
			<xs:element ref="syllable"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="model.syllableLike_sequence">
		<xs:sequence>
			<xs:element ref="syllable"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="model.syllableLike_sequenceOptional">
		<xs:sequence>
			<xs:element minOccurs="0" ref="syllable"/>
