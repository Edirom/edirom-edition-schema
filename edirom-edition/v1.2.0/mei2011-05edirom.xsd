		<xs:attributeGroup ref="att.timestamp.musical.attributes"/>
		<xs:attributeGroup ref="att.timestamp.performed.attributes"/>
		<xs:attributeGroup ref="att.staffident.attributes"/>
		<xs:attributeGroup ref="att.controlevent.attribute.layer"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="att.controlevent.attribute.layer">
