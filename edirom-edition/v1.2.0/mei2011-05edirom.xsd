				<xs:documentation>contains
          the end point of a date range.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="att.datable.attribute.notafter">
		<xs:attribute name="notafter" type="data.ISODATE">
			<xs:annotation>
				<xs:documentation>contains
          an upper boundary for an uncertain date.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="att.datable.attribute.notbefore">
		<xs:attribute name="notbefore" type="data.ISODATE">
			<xs:annotation>
				<xs:documentation>contains
          a lower boundary for an uncertain date.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="att.datable.attribute.startdate">
		<xs:attribute name="startdate" type="data.ISODATE">
			<xs:annotation>
				<xs:documentation>contains
          the starting point of a date range.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="att.datapointing.attributes">
		<xs:attributeGroup ref="att.datapointing.attribute.data"/>
