		<xs:attribute name="complete">
			<xs:annotation>
				<xs:documentation>a value
          of 'c' (complete) indicates a metrically complete measure, 'i'
          (incomplete) indicates a measure with not enough beats, while 'o'
          (overfull) is for measures with too many beats.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:token">
					<xs:enumeration value="c">
						<xs:annotation>
							<xs:documentation/>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="i">
						<xs:annotation>
							<xs:documentation/>
						</xs:annotation>
					</xs:enumeration>
					<xs:enumeration value="o">
						<xs:annotation>
							<xs:documentation/>
						</xs:annotation>
					</xs:enumeration>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:attributeGroup>
