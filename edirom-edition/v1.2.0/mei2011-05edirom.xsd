			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="att.coordinated.attribute.lrx">
		<xs:attribute name="lrx" type="xs:integer"> <!-- TODO: wieder ändern auf nonNegativeInteger -->
