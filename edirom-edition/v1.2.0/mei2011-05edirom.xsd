		<xs:attributeGroup ref="att.lang.attribute.xmllang"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="att.lang.attribute.xmllang">
		<xs:attribute ref="xml:lang"/>
	</xs:attributeGroup>
