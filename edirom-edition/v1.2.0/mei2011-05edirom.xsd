		<xs:sequence>
			<xs:element minOccurs="0" maxOccurs="unbounded" ref="ineume"/>
			<xs:element minOccurs="0" maxOccurs="unbounded" ref="uneume"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="model.eventLike.neumes_sequenceRepeatable">
		<xs:sequence>
