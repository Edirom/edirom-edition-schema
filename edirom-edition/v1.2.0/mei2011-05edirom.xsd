	<xs:group name="model.layerPart.neumes">
		<xs:sequence>
			<xs:group ref="model.syllableLike"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="model.layerPart.neumes_alternation">
		<xs:sequence>
			<xs:group ref="model.syllableLike_alternation"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="model.layerPart.neumes_sequence">
		<xs:sequence>
			<xs:group ref="model.syllableLike_sequence"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="model.layerPart.neumes_sequenceOptional">
		<xs:sequence>
			<xs:group minOccurs="0" ref="model.syllableLike_sequenceOptional"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="model.layerPart.neumes_sequenceOptionalRepeatable">
		<xs:sequence>
			<xs:group minOccurs="0" maxOccurs="unbounded" ref="model.syllableLike_sequenceOptionalRepeatable"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="model.layerPart.neumes_sequenceRepeatable">
		<xs:sequence>
			<xs:group maxOccurs="unbounded" ref="model.syllableLike_sequenceRepeatable"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="model.syllableLike">
