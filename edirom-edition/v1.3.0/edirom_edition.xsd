    <!-- NAME -->
    <xs:element name="name">
        <xs:annotation>
            <xs:documentation xml:lang="de">Der Name eines Objektes</xs:documentation>
        </xs:annotation>
        
        <xs:complexType mixed="true">
            <xs:choice>
                <xs:any namespace="http://www.tei-c.org/ns/1.0" processContents="strict" minOccurs="0" maxOccurs="unbounded" />
                <xs:any namespace="http://www.w3.org/1999/xhtml" processContents="strict" minOccurs="0" maxOccurs="unbounded" />
            </xs:choice>
            
            <!-- NAME/@XML:LANG -->
            <xs:attribute ref="xml:lang" />
        </xs:complexType>
    </xs:element>
    
    <xs:element name="label">
        <xs:annotation>
            <xs:documentation xml:lang="de">Das Label einer Objektart</xs:documentation>
        </xs:annotation>
        
        <xs:complexType mixed="true">
            
            <!-- TITLE/@XML:LANG -->
            <xs:attribute ref="xml:lang" />
        </xs:complexType>
    </xs:element>
    
    <!-- NAMES -->
    <xs:element name="names">
        <xs:annotation>
            <xs:documentation xml:lang="de">Meta-Element "Names", dass die internationalisierten Namen aufnimmt</xs:documentation>
        </xs:annotation>
        
        <xs:complexType mixed="false">
            <xs:sequence minOccurs="1" maxOccurs="unbounded">
                <xs:element ref="name" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation xml:lang="de">Ein Name eines Objekts</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="labels">
        <xs:annotation>
            <xs:documentation xml:lang="de">Meta-Element "labels", dass die internationalisierten Label aufnimmt</xs:documentation>
        </xs:annotation>
        
        <xs:complexType mixed="false">
            <xs:sequence minOccurs="1" maxOccurs="unbounded">
                <xs:element ref="label" maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation xml:lang="de">Ein Label einer Objektart</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    
