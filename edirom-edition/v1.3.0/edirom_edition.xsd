                                        <xs:element name="category" maxOccurs="unbounded">
                                            <xs:annotation>
                                                <xs:documentation xml:lang="de">Eine Kategorie einer Anmerkung</xs:documentation>
                                            </xs:annotation>
                                            
                                            <xs:simpleType>
                                                <xs:annotation>
                                                    <xs:documentation xml:lang="de">Die gezeigten Anmerkungskategorien sind lediglich Beispiele bzw. Vorschläge</xs:documentation>
                                                </xs:annotation>
                                                <xs:union>
                                                    <xs:simpleType>
                                                        <xs:restriction base="xs:string">
                                                            <xs:enumeration value="Bogensetzung" />
                                                            <xs:enumeration value="Verbal instruction" />
                                                            <xs:enumeration value="Embellishment" />
                                                            <xs:enumeration value="Correction" />
                                                            <xs:enumeration value="Diastematic mark" />
                                                            <xs:enumeration value="Dynamics" />
                                                            <xs:enumeration value="Harmony" />
                                                            <xs:enumeration value="Rhythm" />
                                                            <xs:enumeration value="Articulation" />
                                                            <xs:enumeration value="Editorial intervention" />
                                                            <xs:enumeration value="Inclusion from A" />
                                                            <xs:enumeration value="Notational peculiarity" />
                                                            <xs:enumeration value="Notational variant" />
                                                            <xs:enumeration value="Notational imprecision" />
                                                            <xs:enumeration value="Alteration by analogy" />
                                                            <xs:enumeration value="Stemmatic Relationship" />
                                                            <xs:enumeration value="Separative Variant" />
                                                            <xs:enumeration value="Conjunctive Error" />
                                                            <xs:enumeration value="Score" />
                                                            <xs:enumeration value="Direction" />
                                                            <xs:enumeration value="Lyrics" />
                                                            <xs:enumeration value="Text" />
                                                            <xs:enumeration value="Music" />
                                                            <xs:enumeration value="Stage" />
                                                            <xs:enumeration value="Scene" />
                                                        </xs:restriction>
                                                    </xs:simpleType>
                                                    <xs:simpleType>
                                                        <xs:restriction base="xs:string">
                                                            <xs:pattern value=".*"/>
                                                        </xs:restriction>
                                                    </xs:simpleType>
                                                </xs:union>
                                            </xs:simpleType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            
                            <!-\- CONTENTS -\->
                            <xs:element name="contents" minOccurs="1">
                                <xs:annotation>
                                    <xs:documentation xml:lang="de">Der Inhalt einer Anmerkung betseht aus einem im tei-Namespace geschriebenen Text kombiniert mit der Angabe weiterer anzuzeigender Takte</xs:documentation>
                                </xs:annotation>
                                
                                <xs:complexType mixed="true">
                                    <xs:sequence>
                                        
                                        <!-\- CONTENT -\->
                                        <xs:element name="content" minOccurs="1">
                                            <xs:annotation>
                                                <xs:documentation xml:lang="de">Der Content besteht aus einem oder mehreren (versch. Sprachen) TEI-Elementen</xs:documentation>
                                            </xs:annotation>
                                            
                                            <xs:complexType mixed="true">
                                                <xs:sequence>
                                                    
                                                    <!-\- ANY_TEI -\->
                                                    <xs:any namespace="http://www.tei-c.org/ns/1.0" processContents="strict" minOccurs="1" maxOccurs="1" />
                                                    
                                                </xs:sequence>
                                                
                                                <xs:attribute ref="xml:lang" />
                                            </xs:complexType>
                                        </xs:element>
                                        
                                        <!-\- FACSIMILE-FLOW -\->
                                        <xs:element ref="facsimileFlow" minOccurs="0" maxOccurs="unbounded"/>
                                        
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:all>
                        
                        
                        <!-\- FIRST-BAR -\->
                        <xs:attribute name="firstBar">
                            <xs:annotation>
                                <xs:documentation xml:lang="de">Der erste Takt der Anmerkung aus dem angegeben Satz</xs:documentation>
                            </xs:annotation>
                            
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:pattern value="(U-)?[0-9]+[a-z]?" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:attribute>
                        
                        <!-\- LAST-BAR -\->
                        <xs:attribute name="lastBar">
                            <xs:annotation>
                                <xs:documentation xml:lang="de">Der letzte Takt der Anmerkung aus dem angegeben Satz</xs:documentation>
                            </xs:annotation>
                            
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:pattern value="(U-)?[0-9]+[a-z]?" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:attribute>
                        
                        <!-\- RELEVANCE -\->
                        <xs:attribute name="relevance" use="required">
                            <xs:annotation>
                                <xs:documentation xml:lang="de">Relevanzkategorien: Zahlenwert</xs:documentation>
                            </xs:annotation>
                            
                            <xs:simpleType>
                                <xs:restriction base="xs:int">
                                    <xs:pattern value="[0-3]" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:attribute>
                        
                        <!-\- SOURCES -\->
                        <xs:attribute name="sources">
                            <xs:annotation>
                                <xs:documentation xml:lang="de">Hier stehen alle Referenzen zu den Quellen</xs:documentation>
                            </xs:annotation>
                            
                            <xs:simpleType>
                                <xs:restriction base="xs:IDREFS">
                                    <xs:pattern value="(source-[0-9]+\s)*(source-[0-9]+)" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:attribute>
                        
                        <!-\- VOICES -\->
                        <xs:attribute name="voices">
                            <xs:annotation>
                                <xs:documentation xml:lang="de">Hier stehen alle Referenzen zu den Stimmen</xs:documentation>
                            </xs:annotation>
                            
                            <xs:simpleType>
                                <xs:restriction base="xs:IDREFS">
                                    <xs:pattern value="(voice-[0-9]+\s)*(voice-[0-9]+)" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:attribute>
                        
                        <!-\- PART -\->
                        <xs:attribute name="part">
                            <xs:annotation>
                                <xs:documentation xml:lang="de">Hier steht die Referenz zu dem Part</xs:documentation>
                            </xs:annotation>
                            
                            <xs:simpleType>
                                <xs:restriction base="xs:IDREF">
                                    <xs:pattern value="part-[0-9]+" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:attribute>
                        
                        <!-\- GENERAL-REMARK -\->
                        <xs:attribute name="generalRemarks">
                            <xs:annotation>
                                <xs:documentation xml:lang="de">Hier stehen alle Referenzen zu den Generellen Anmerkungen</xs:documentation>
                            </xs:annotation>
                            
                            <xs:simpleType>
                                <xs:restriction base="xs:IDREFS">
                                    <xs:pattern value="(generalRemark-[0-9]+\s)*(generalRemark-[0-9]+)" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:attribute>
                        
                        <!-\- ANNOTATION/@ID -\->
                        <xs:attribute ref="xml:id" use="required" />
                        
                        <!-\- ANNOTATION/@SORT-NO -\->
                        <xs:attribute name="sortNo" use="required" type="xs:int" />
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>-->
    
