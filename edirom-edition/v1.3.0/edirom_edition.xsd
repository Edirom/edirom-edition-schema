    <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="http://www.w3.org/2001/xml.xsd">
        <xs:annotation>
            <xs:documentation xml:lang="de">Der XML-Namespace bietet Standard--Attribute für IDs und Sprachangaben</xs:documentation>
        </xs:annotation>
    </xs:import>
    
    <xs:import namespace="http://www.w3.org/1999/xlink" schemaLocation="xlink.xsd" />
    
    <xs:import namespace="http://www.w3.org/1999/xhtml" schemaLocation="xhtml1-strict.xsd" />
    
    
    <!-- EDITION -->
    <xs:element name="edition">
        <xs:annotation>
            <xs:documentation xml:lang="de">Das Wurzelelement einer Edition für die Edirom. Es beinhaltet sowohl Metaangaben zu der Edition selbst, als auch die in der Edition behandelten Werke.</xs:documentation>
        </xs:annotation>
        
        <xs:complexType mixed="true">
            <xs:all>

                <!-- EDITION-NAME -->
                <xs:element name="editionName" minOccurs="1">
                    <xs:annotation>
                        <xs:documentation xml:lang="de">Name der Edition</xs:documentation>
                    </xs:annotation>
                </xs:element>
                
                <!-- LANGUAGES -->
                <xs:element name="languages" minOccurs="1">
                    <xs:annotation>
                        <xs:documentation xml:lang="de">Die zur Verfügung stehenden Sprachen mit Angaben zur Vollständigkeit</xs:documentation>
                    </xs:annotation>
                    
                    <xs:complexType>
                        <xs:sequence>
                            
                            <!-- LANGUAGE -->
                            <xs:element name="language" minOccurs="1" maxOccurs="unbounded">
                                <xs:annotation>
                                    <xs:documentation xml:lang="de">Angabe einer in der Edition genutzten Sprache</xs:documentation>
                                </xs:annotation>
                                
                                <xs:complexType mixed="true">
                                    <xs:attribute ref="xml:lang" use="required" />
                                    <xs:attribute name="complete" type="xs:boolean" use="required" />
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                
                <!-- PREFERENCES -->
                <xs:element name="preferences" minOccurs="0" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation>Referenz auf eine Edirom Online preferences Datei.</xs:documentation>
                    </xs:annotation>
                    
                    <xs:complexType>
                        <xs:attribute ref="xlink:href" use="required"/>
                    </xs:complexType>
                </xs:element>
                
                <!-- WORKS -->
                <xs:element ref="works" minOccurs="1" maxOccurs="1">
                    <xs:annotation>
                        <xs:documentation xml:lang="de">Die in der Edition enthaltenen Werke</xs:documentation>
                    </xs:annotation>
                </xs:element>
                
            </xs:all>
            
            <!-- EDITION/@ID -->
            <xs:attribute ref="xml:id" use="required">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Die ID der Edition setzt sich aus dem Präfix 'edition-' und einer 8-stelligen Zahl zusammen und sollte zumindest im Projektkontext eindeutig sein</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    
