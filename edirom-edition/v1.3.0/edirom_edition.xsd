    <!-- Konkordanzen -->
    <xs:element name="concordances">
        <xs:complexType mixed="false">
            <xs:sequence>
                <xs:element name="concordance" minOccurs="1" maxOccurs="unbounded">
                    <xs:complexType mixed="false">
                        <xs:sequence>
                            <xs:element minOccurs="0" maxOccurs="1" ref="names"/>
                            <xs:choice maxOccurs="1">
                                <xs:element ref="groups"/>
                                <xs:element ref="connections"/>
                            </xs:choice>
                        </xs:sequence>
                        
                        <xs:attribute name="name" type="xs:string"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="groups">
        <xs:complexType mixed="false">
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="1" ref="labels"/>
                <xs:element name="group" minOccurs="1" maxOccurs="unbounded">
                    <xs:complexType mixed="false">
                        <xs:sequence>
                            <xs:element minOccurs="0" maxOccurs="1" ref="names"/>
                            <xs:element minOccurs="1" maxOccurs="1" ref="connections"/>
                        </xs:sequence>

                        <xs:attribute name="name" type="xs:string"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            
            <xs:attribute name="label" type="xs:string"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="connections">
        <xs:complexType mixed="false">
            <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="1" ref="labels"/>
                <xs:element name="connection" minOccurs="1" maxOccurs="unbounded">
                    <xs:complexType mixed="false">
                        <xs:sequence>
                            <xs:element minOccurs="0" maxOccurs="1" ref="names"/>
                        </xs:sequence>
                        
                        <xs:attribute name="name" type="xs:string"/>
                        <xs:attribute name="plist" use="required">
                            <xs:simpleType>
                                <xs:union>
                                    <xs:simpleType>
                                        <xs:restriction base="xs:anyURI"/>
                                    </xs:simpleType>
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                            <xs:whiteSpace value="preserve"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:union>
                            </xs:simpleType>
                        </xs:attribute>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            
            <xs:attribute name="label" type="xs:string"/>
        </xs:complexType>
    </xs:element>
    
    <!-- Flexible Navigator -->
    <xs:attributeGroup name="att.sorting">
        <xs:attribute name="sortNo" type="xs:int" use="required"/>
    </xs:attributeGroup>
    
    <xs:group name="navigatorItemOrSeparator">
        <xs:choice>
            <xs:element ref="navigatorItem" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="navigatorSeparator" minOccurs="0" maxOccurs="1"/>
        </xs:choice>
    </xs:group>
    
    <xs:group name="navigatorContents">
        <xs:choice>
            <xs:element ref="navigatorCategory" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="navigatorItem" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="navigatorSeparator" minOccurs="0" maxOccurs="1"/>
        </xs:choice>
    </xs:group>
    
    <xs:element name="navigatorCategory">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="names" minOccurs="1" maxOccurs="1"/>
                <xs:group ref="navigatorContents" minOccurs="1" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute ref="xml:id" use="required" />
            <xs:attributeGroup ref="att.sorting"/>
            <xs:attribute name="targets" type="xs:anyURI" use="optional"/>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="navigatorItem">
        <xs:complexType mixed="true">
            <xs:all>
                <xs:element ref="names" minOccurs="1"/>
            </xs:all>
            <xs:attribute ref="xml:id" use="required"/>
            <xs:attributeGroup ref="att.sorting"/>
            <xs:attribute name="targets" type="xs:anyURI" use="required"/>
        </xs:complexType>
    </xs:element>
   
    <xs:element name="navigatorSeparator">
        <xs:complexType mixed="false">
            <xs:attributeGroup ref="att.sorting"/>
        </xs:complexType>
    </xs:element>
    
</xs:schema>
